{
  "name": "opencart/opencart",
  "type": "project",
  "description": "OpenCart",
  "keywords": ["opencart", "ecommerce", "framework", "opensource"],
  "homepage": "https://www.opencart.com",
  "license": "GPL-3.0+",
  "config": {
    "vendor-dir": "./upload/system/storage/vendor/",
    "platform":{
      "php": "7.4.33"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\Command\\": "./command/",
      "App\\Command\\Services\\": "./bin/services",
      "App\\System\\": "./system/",
      "App\\System\\Library\\": "./system/library/",
      "Multivers\\": "./system/library/multivers/",
      "Multivers\\Classes\\": "./system/library/multivers/classes/"
    }
  },
  "require": {
    "cardinity/cardinity-sdk-php": "^3",
    "braintree/braintree_php" : "3.40.0",
    "scssphp/scssphp": "1.1.1",
    "divido/divido-php": ">=1.1.1",
    "php": ">=5.4.0",
    "twig/twig": "^2.4.8",
    "zoujingli/wechat-php-sdk": ">=1.3.10",
    "katzien/php-mime-type": "2.1.0",
    "hassankhan/config": "2.0.2",
    "noodlehaus/config": "@stable",
    "thunderer/shortcode": "v0.7.2",
    "guzzlehttp/guzzle": "7.7.0",
    "seld/jsonlint": "1.8.0",
    "erusev/parsedown": "^1.7",
    "erusev/parsedown-extra": "^0.8.1",
    "ext-json": "*",
    "vlucas/phpdotenv": "v5.6.0",
    "voku/html-min": "^4.5",
    "voku/html-compress-twig": "^4.0",
    "owasp/csrf-protector-php": "@stable",
    "league/route": "@stable",
    "laminas/laminas-diactoros": "@stable",
    "ext-curl": "*",
    "ext-mysqli": "*",
    "minicli/minicli": "^2.2"
  },
  "require-dev": {
    "spatie/ray": "~1.41.2",
    "beyondcode/tinkerwell": "@dev"
  }
}
